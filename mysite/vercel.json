{
    "version": 2,
    "builds": [
      {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "mysite/wsgi.py" }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "mysite.settings"
    },
    "functions": {
      "api/**/*.js": {
        "runtime": "@vercel/node"
      }
    }
  }
  